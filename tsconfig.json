{
  "compileOnSave": false,
  "compilerOptions": {
    "lib": [
      "es6",
      "dom",
      "es2016"
    ],
    "target": "es2015",
    "module": "commonjs",
    "noImplicitReturns": false,
    // 允许 any 类型
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true
  }
}